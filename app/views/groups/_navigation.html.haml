.navbar-left.left
  .navbar-left__user
    %p.left chat-ojisan
    .items.right
      = link_to new_group_path, class: 'icon-btn' do
        %i.fa.fa-pencil-square-o
      = link_to edit_user_registration_path, class: 'icon-btn' do
        %i.fa.fa-cog
  .navbar-left__groups
    %ul
      - current_user.groups.each do |group|
        %li.list-group
          = link_to group_messages_path(group) do
            %p.list-group__name
              = group.name
            %p.list-group__message
              aaaaaaaaaaaaa
